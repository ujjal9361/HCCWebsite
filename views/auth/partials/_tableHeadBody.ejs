<thead>
    <tr>
      <th>SN</th>
      <th>Name</th>
      <th>Email</th>
      <th>Phone Number</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>
  <!-- Table body is just one but each table row is a different student -->
  <tbody>
    <%users.forEach((user,index)=>{%>
      <tr>
        <td><%=index+1%></td>
        <td><%=user.fullName%></td>
        <td><%=user.email%></td>
        <td><%=user.phoneNumber%></td>
        <td><i class="fa fa-pencil-square-o" aria-hidden="true"></i></td>
        <td><i class="fa fa-trash-o" aria-hidden="true"></i></td>
      </tr>
    <%})%>
  </tbody>