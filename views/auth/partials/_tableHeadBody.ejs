<thead>
    <tr>
      <th>SN</th>
      <th>Name</th>
      <th>Email</th>
      <th>Phone Number</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>
  <!-- Table body is just one but each table row is a different student -->
  <tbody>
    <%users.forEach((user,index)=>{%>
      <tr>
        <td><%=index+1%></td>
        <td><%=user.fullName%></td>
        <td><%=user.email%></td>
        <td><%=user.phoneNumber%></td>
        <td><a href="<%=`/admin/users/${user._id}/edit`%>"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></a></td>
        <td><form action="<%=`/admin/users/${user._id}?_method=DELETE` %>" method="POST">
          <button type="button" ><i class="fa fa-trash-o" aria-hidden="true" onclick="askToDeleteEvent(this.parentNode.parentNode)"></i></button>
        </form></td>
      </tr>
    <%})%>
  </tbody>